{
  "env": {
    "node": true,
    "mocha": true,
    "jest": true
  },
  "parserOptions": {
    "ecmaVersion": 9
  },
  "extends": [
    "astro",
    "prettier",
    "plugin:prettier/recommended",
    "eslint-config-prettier"
  ],
  "settings": {
    "import/resolver": "module-alias"
  },
  "rules": {
    "import/no-extraneous-dependencies": "off",
    "no-param-reassign":"off"
  }
}
